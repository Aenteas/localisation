from map import Map
from map import Robot
import numpy as np
from map import Map
import random
from localization import Localization
import math

num_features = 30
map_size = (500, 600)
min_timediff_sec = 1.0/50.0
#generate features
features = []
for i in range(num_features):
        pos_x = np.random.rand() * map_size[1]
        pos_y = np.random.rand() * map_size[0]
        features.append((pos_x, pos_y))

map = Map(Robot(pos=(300, 250)), features, Localization(min_timediff_sec))
map.simulate()
